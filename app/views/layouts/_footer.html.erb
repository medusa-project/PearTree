<footer class="container-fluid s-lib-side-borders" role="contentinfo">
  <div class="row lg-feedback-footer">
    <div class="col-md-12 feedback-footer" aria-label="Feedback info">
      <p><%= feedback_link %></p>
    </div>
  </div>
  <div class="row lg-copyright-footer">
    <div class="col-md-12 copyright-footer" aria-label="university library copyright info">
      <p>
        <a href="http://illinois.edu" aria-label="University of Illinois website link">
          <%= image_tag('Illinois-Logo-Reversed-Blue-RGB-100.png',
                        width: 35, height: 50,
                        alt: 'University of Illinois Logo') %>
        </a>
      </p>
      <p><%= Option::string(Option::Keys::COPYRIGHT_STATEMENT) %></p>
    </div>
  </div>
</footer>