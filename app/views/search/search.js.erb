$('#dl-items').html('<%= escape_javascript(render('entities')) %>');
$('.dl-facets').html('<%= escape_javascript(render('facets')) %>');
$('#dl-search-status').html('<%= search_status(@count, @start, @num_results_shown) %>');

<%
atom_url = search_path(@permitted_params.merge(format: 'atom'))
json_url = search_path(@permitted_params.merge(format: 'json'))
%>

$('#dl-view-as-atom').attr('href', '<%= atom_url %>');
$('#dl-view-as-json').attr('href', '<%= json_url %>');

Application.view.attachEventListeners();